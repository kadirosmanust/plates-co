{
  "name": "plates-co",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "prestart": "node ./internals/scripts/checkNodeVersion.cjs",
    "start": "NODE_ENV=development webpack serve --mode development --config ./webpack.dev.config.js",
    "build": "NODE_ENV=production NODE_OPTIONS=--max_old_space_size=5120 webpack --mode production --color --progress --config ./webpack.prod.config.js",
    "serve-build": "http-server ./build -p 9003 --gzip --proxy http://localhost:9003?",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint --fix src/**/*.js",
    "test": "jest --runInBand",
    "test-coverage": "npm run test -- --coverage",
    "test-ci": "npm run test-coverage -- --silent --ci --bail",
    "test-watch": "npm run test -- --watch",
    "test-changed": "npm run test-coverage -- --changedSince=origin/master --silent",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.24.8",
    "@babel/plugin-proposal-export-default-from": "^7.24.7",
    "@babel/preset-env": "^7.24.8",
    "babel-loader": "^9.1.3",
    "babel-plugin-import": "^1.13.8",
    "compression-webpack-plugin": "^11.1.0",
    "http-server": "^14.1.1",
    "sass": "^1.77.8",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "thread-loader": "^4.0.2",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^6.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.24.8",
    "@babel/plugin-transform-modules-commonjs": "^7.24.8",
    "@testing-library/jest-dom": "5.16.4",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "chalk": "4.1.2",
    "css-loader": "^7.1.2",
    "eslint": "8.37.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-webpack": "^0.13.8",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-no-inline-styles": "^1.0.5",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-testing-library": "^6.2.2",
    "eslint-webpack-plugin": "^4.2.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "27.5.1",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^9.6.0",
    "semver": "7.3.7",
    "webpack-dev-server": "^5.0.4",
    "husky": "^8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.1%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "verbose": true,
    "testTimeout": 15000,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageDirectory": "./coverage/",
    "coverageThreshold": {
      "global": {
        "branches": 20,
        "functions": 20,
        "lines": 20,
        "statements": 20
      }
    },
    "moduleDirectories": [
      "node_modules",
      "src",
      "tests"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/', '<rootDir>/build/"
    ],
    "testMatch": [
      "**/*.(spec|test).js"
    ]
  }
}
